<p id="notice"><%= notice %></p>

<h1>Records</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Age group</th>
      <th>Gender</th>
      <th>Stroke</th>
      <th colspan="1"></th>
    </tr>
  </thead>

  <tbody>
    <% @records.each do |record| %>
      <tr>
        <td><%= record.age_group %></td>
        <td><%= record.gender %></td>
        <td><%= record.stroke %></td>
        <td><%= link_to 'Edit', edit_record_path(record) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record', new_record_path %>
